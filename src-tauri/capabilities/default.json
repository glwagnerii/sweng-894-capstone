{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "os:default",
    "store:default",
    "sql:default",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://fakestoreapi.com" }]
    },
    "opener:default",
    "notification:default",
    "fs:default",
    "dialog:default"
  ]
}
